var getinfo=require("readline-sync");
console.log("This is the if else condition");
var get1 = getinfo.question("Enter 1st Number: ");
var get2 = getinfo.question("Enter 2nd Number: ");
console.log("This is the  1st Number You Have Entered :" +get1);
console.log("This is the  2nd Number You Have Entered :" +get2);
if(get1 > get2)
{
console.log("1st Number Is Greater");
}
else if(get1 ==get2)
{
console.log("Both Number's Are Same");
}
else
{
console.log("2nd Number Is Greater");
}

